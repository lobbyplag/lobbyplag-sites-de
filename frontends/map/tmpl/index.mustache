	{{> footer}}

<div id="mailToolModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalMailToolLabel"
	 aria-hidden="true">
	<div class="modal-header">
		<h3 id="myModalMailToolLabel">Schicke eine E-Mail an die Abgeordneten</span>
			<button data-dismiss="modal" class="btn pull-right"><i class="icon-remove"></i></button>
		</h3>
	</div>
	<div id="mailtool_body" class="modal-body">
		<iframe id="mailtool" src=""></iframe>
	</div>
</div>
{{#enableReport}}
<div id="reportErrorModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalJumpLabel"
	 aria-hidden="true">
	<div class="modal-header">
		<h3 id="myModalJumpLabel">Änderung der Bewertung des Änderungsantrages #<span id="current_amend"></span></h3>
	</div>
	<div class="modal-body">
		Die derzeitige Bewertung ist: <span id="current_rating"></span><br/>
		<br/>
		Deine Bewertung wäre:
		<form class="form-inline" id="report_vote">
			<fieldset>
				<label class="radio">
					<input type="radio" name="vote" value="weaker">
					schwächer
				</label>
				<label class="radio">
					<input type="radio" name="vote" value="stronger">
					stärker
				</label>
				<label class="radio">
					<input type="radio" name="vote" value="neutral">
					neutral
				</label>
			</fieldset>
		</form>
		Dein Name (Optional):<br/>
		<input id="report_name" type="text" placeholder=""><br/><br/>
		Kommentar (Erforderlich)
		<textarea id="report_comment"></textarea>
	</div>
	<label id="report_info" class="alert alert-info hidden"></label>

	<div class="modal-footer">
		<button id="btn_send" class="btn btn-primary">Senden</button>
		<button data-dismiss="modal" class="btn btn-primary">Abbrechen</button>
	</div>
</div>
{{/enableReport}}


</body>

</html>
