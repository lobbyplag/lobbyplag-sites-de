<article id="result" class="result">
	{{#result}}
		<div class="row">
			<div class="span6">
				<h2>Lobbypapiere</h2>
				{{#lob}}
					<section class="result">
						<h3>{{lobbyist_title}}</h3>
						<div class="diff-container">
							<div class="diff-buttons btn-group">
								<a class="btn btn-mini btn-inverse button-old" href="javascript:;">Verodnungsentwurf</a>
								<a class="btn btn-mini btn-primary button-all" href="javascript:;">Unterschiede</a>
								<a class="btn btn-mini btn-inverse button-new" href="javascript:;">Geänderte Version</a>
							</div>
							<div class="diff">{{{diff}}}</div>
						</div>
						<div class="source">
							<div class="display-source">
								<a href="{{lobbyist_url}}" class="lobbyist">{{lobbyist_title}}</a>
								<a title="Dokuent herunterladen" class="badge badge-info" href="{{url}}#page={{page}}">Quelle</a>
								<span class="badge badge-info">Seite {{page}}</span></div>
							<div class="clear"></div>
						</div>
					</section>
				{{/lob}}
				{{^lob}}
					<section>
						<h3>Keine Ergebnisse</h3>
						<p>In den Lobbypapieren wurden keine Übereinstimmungen gefunden.</p>
					</section>
				{{/lob}}
			</div>
			<div class="span6">
				<h2>Änderungsvorschläge</h2>
				{{#amd}}
					<section class="result">
						<h3>{{committee}} Änderungsvorschlag #{{number}}</h3>
						<p class="location">{{location}}</p>
						{{#text}}
						<div class="diff-container">
							<div class="diff-buttons btn-group">
								<a class="btn btn-mini btn-inverse button-old" href="javascript:;">Verodnungsentwurf</a>
								<a class="btn btn-mini btn-primary button-all" href="javascript:;">Unterschiede</a>
								<a class="btn btn-mini btn-inverse button-new" href="javascript:;">Geänderte Version</a>
							</div>
							<div class="diff">{{{diff}}}</div>
						</div>
						{{/text}}
						<p class="authors"><strong>Autoren:</strong>{{#authors}} {{.}};{{/authors}}</p>
					</section>
				{{/amd}}
				{{^amd}}
					<section>
						<h3>Keine Ergebnisse</h3>
						<p>In den Änderungsvorschlägen wurden keine Übereinstimmungen gefunden.</p>
					</section>
				{{/amd}}
			</div>
		</div>
	{{/result}}
	{{^result}}
		<section>
			<h2>Keine Ergebnisse</h2>
			<p>Es wurden keine Übereinstimmungen gefunden.</p>
		</section>
	{{/result}}
</article>